<core:FragmentDefinition
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core">

<m:VBox id="_IDGenVBox1">
    <m:HBox id="_IDGenHBox2">
        <m:Label id="_IDGenLabel" text="Supplier Name : " labelFor="_IDGenText14" />
        <m:Text id="_IDGenText14" text="{Supplier/Name}" />
    </m:HBox>
    <m:HBox id="_IDGenHBox3">
        <m:Label id="_IDGenLabel5" text="Street : " labelFor="_IDGenText17" />
        <m:Text id="_IDGenText17" text="{Supplier/Address/Street}" />
    </m:HBox>
    <m:HBox id="_IDGenHBox4">
        <m:Label id="_IDGenLabel9" text="City : " labelFor="_IDGenText18" />
        <m:Text id="_IDGenText18" text="{Supplier/Address/City}" />
    </m:HBox>
    <m:HBox id="_IDGenHBox5">
        <m:Label id="_IDGenLabel10" text="State : " labelFor="_IDGenText19" />
        <m:Text id="_IDGenText19" text="{Supplier/Address/State}" />
    </m:HBox>
    <m:HBox id="_IDGenHBox6">
        <m:Label id="_IDGenLabel11" text="ZipCode : " labelFor="_IDGenText20" />
        <m:Text id="_IDGenText20" text="{Supplier/Address/ZipCode}" />
    </m:HBox>
    <m:HBox id="_IDGenHBox7">
        <m:Label id="_IDGenLabel12" text="Country : " labelFor="_IDGenText21" />
        <m:Text id="_IDGenText21" text="{Supplier/Address/Country}" />
    </m:HBox>
</m:VBox>

</core:FragmentDefinition>